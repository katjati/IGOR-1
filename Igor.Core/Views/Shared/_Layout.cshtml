@using Igor.Library.Models.Shared
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - IGOR</title>
    <link rel="SHORTCUT ICON" href="~/favicon.ico" />
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/modernizr")


</head>
<body>
    <div class="igor-container container-vertical">
        <div class="igor-header modal-bg">
            @Html.Partial("_Header")
        </div>
        <div class="terminal-body igor-container center" id="terminal-content">
            @RenderBody()
        </div>
    </div>


@RenderSection("scripts", required: false)
</body>
</html>
